{
  "name": "pacientech-api",
  "version": "0.1.0",
  "description": "A RESTful api to view, add, modify, and delete patient records for private practicing physicians.",
  "scripts": {
    "start": "NODE_ENV=production node production/server.js",
    "build": "rm -rf ./production/ && babel src -d production --ignore 'src/**/*.test.js'",
    "dev": "NODE_ENV=development nodemon --exec babel-node src/server.js",
    "lint": "eslint src/**/*.js",
    "test": "NODE_ENV=test mocha --require @babel/register --colors --exit src/**/*.test.js"
  },
  "main": "src/server.js",
  "repository": "https://github.com/vzhny/pacientech-api",
  "author": "Diego Vizhnay <diego@vzhny.com> (https://vizhnay.io)",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint",
      "pre-push": "yarn run test"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "babel-plugin-dotenv": "^0.1.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.24.0",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.9",
    "morgan": "^1.9.1",
    "shortid": "^2.2.14",
    "time-stamp": "^2.2.0",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "husky": "^1.3.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "supertest": "^3.4.2"
  }
}
